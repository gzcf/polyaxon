{
  "name": "polyaxon-ui",
  "version": "0.0.1",
  "description": "Polyaxon ui",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/* && rm -rf public/dist/* && mkdir -p public/dist/js && mkdir public/dist/css && mkdir public/dist/fonts",
    "_copy_react": "cp node_modules/react/umd/react.production.min.js node_modules/react-dom/umd/react-dom.production.min.js public/dist/js",
    "_copy_css": "cp node_modules/font-awesome/css/font-awesome.min.css node_modules/bootstrap/dist/css/bootstrap.min.css public/dist/css",
    "_copy_fonts": "cp -r node_modules/font-awesome/fonts public/dist/",
    "copy": "npm run _copy_react && npm run _copy_css && npm run _copy_fonts",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output public/dist/js/bundle.min.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --port 8001 --env.dev --config webpack.config.dev.js  --output-public-path=dist/",
    "build": "npm run clean && webpack --env.prod --config webpack.config.prod.js && npm run minify && npm run copy"
  },
  "author": "mourad mourafiq",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^8.0.19",
    "@types/redux-logger": "^3.0.0",
    "awesome-typescript-loader": "^3.2.2",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "fontawesome": "^4.7.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.5.0",
    "tslint-react": "^3.4.0",
    "typescript": "^2.4.2",
    "uglify-js": "^3.3.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "@types/js-cookie": "^2.1.0",
    "@types/lodash": "^4.14.71",
    "@types/react": "^16.0.36",
    "@types/react-bootstrap": "^0.32.2",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.21",
    "@types/react-router-bootstrap": "^0.24.3",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux-form": "^7.0.13",
    "@types/redux-thunk": "^2.1.0",
    "@types/sprintf-js": "0.0.27",
    "bootstrap": "^3.3.7",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "normalizr": "^3.2.3",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "~2.2.0",
    "sprintf-js": "^1.1.1",
    "whatwg-fetch": "^2.0.3"
  }
}
